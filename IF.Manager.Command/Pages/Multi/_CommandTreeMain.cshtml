@model IF.Manager.Contracts.Dto.CommandMapModel


    <script>

        function setselectedtreenode() {
            //debugger;
            var id = $("#CommandMapTree").jstree('get_selected');
            //alert(id);
            if (id == "") {
                alert("Lütfen bir eleman seçiniz.")
            }

            var data = {};

            data.ParentId = id[0];
            //debugger;
            return data;
        }


        $(document).ready(function () {
            $("#CommandMapTree").jstree({
                "plugins": ["wholerow"]
            })
        });
    </script>

@{await Html.RenderPartialAsync("_CommandTreeList.cshtml", Model);}