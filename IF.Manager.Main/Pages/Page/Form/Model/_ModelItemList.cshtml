@model List<IF.Manager.Contracts.Model.IFPageControlItemModelProperty>

<table class="table table-striped table-sm" id="EmptyFormItem">
    <thead>
        <tr>
            <th>Property</th>
            <th>Control</th>
            <th>Query</th>
            <th>Sequence</th>
        </tr>
    </thead>

    @for (int i = 0; i < Model.Count; i++)
