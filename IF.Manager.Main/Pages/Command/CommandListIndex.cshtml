@page
@model IF.Manager.Command.Pages.CommandListIndexModel

<div class="row">
    <div class="col-md-12">
        <a class="btn btn-success"
           href="@Url.Page("/Command/CommandForm","Add")"
           if-ajax="true"
           if-ajax-method="get"
           if-ajax-insertion-mode="replace"
           if-ajax-show-dialog="true"
           if-ajax-modal-id="@Guid.NewGuid()">
            Add
        </a>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <hr />
    </div>
</div>

<div class="row">
    <div id="CommandListDiv" class="col-md-12">
        @{await Html.RenderPartialAsync("_CommandListTable", Model.CommandList);}
    </div>
</div>
